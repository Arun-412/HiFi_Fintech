RewriteEngine on

RewriteCond %{HTTP_REFERER} !^$
RewriteCond %{HTTP_REFERER} !^http://api.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://api.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^http://dev.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://dev.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^http://hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.api.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.api.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.dev.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.dev.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^https://api.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://api.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^https://dev.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://dev.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^https://hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.api.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.api.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.dev.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.dev.hififintech.com$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.hififintech.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.hififintech.com$      [NC]
RewriteRule .*\.(jpg|jpeg|gif|png|bmp)$ https://hififintech.com/ [R,NC]

